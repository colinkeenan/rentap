var butt = document.createElement("button");    //define button element
var btext = document.createTextNode("Put Form data in URL to be bookmarked");  //define the text
butt.appendChild(btext);                        //attach text to the button

butt.addEventListener("click",                  //handle onclick event
   function(){   
      self.postMessage("go");
   },
false);

document.getElementById("urlbutton").appendChild(butt); //put the button on the page
